rem(px)
  return px/26rem
.search
  position relative
  background-color #f8f8f8
  padding .325rem .5rem
  p
    box-sizing border-box
    width 100%
    height 1.5rem
    line-height 1.5rem
    background url(../images/icon_22.png)no-repeat
    background-position 4% 50%
    background-color white
    padding-left 1.65rem
    border-radius 1.5rem
    color #999
    outline none
    font-size rem(13)
.one-list
  width 100%
  height 1.75rem
  background white
  border-bottom 2px solid #f8f8f8
  li
    float left
    height .75rem
    width 25%
    box-sizing border-box
    font-size .5rem
    border-left 1px solid #e6e6e6
    color #e6e6e6
    text-align center
    line-height .75rem
    margin-top .5rem
  :first-child
    border-left none
  .one-list-black
    color #333333
.two-list
  width 100%
  display none
  float left
.two-lists
  float left
  width 45%
  box-sizing border-box
  li
    height 2.225rem
    line-height 2.225rem
    padding 0 1rem
    font-size .6rem
    //background #f8f8f8
    a
      color black
.two-listSha
  display block
